
tool_details_start
   name          "ImageInfo";
   version       "1.00 (01-Jul-23)";
   wimpslot      64k;
   has_extended_cmdline;
tool_details_end


metaoptions_start
   has_auto_run         on;
   has_text_window;
   has_summary_window;
metaoptions_end
   

dbox_start
   icons_start
      icn 3 maps_to string  not_saved;
   icons_end

   defaults
   icn 3 "";

   imports_start
      drag_to icn 3   inserts icn 3 separator_is " ";
      drag_to any     inserts icn 3 separator_is " ";
      drag_to iconbar inserts icn 3;
   imports_end
dbox_end
